{% extends 'base.html.twig' %}

{% block body %}

<div class="example-wrapper">
    <ul>
        {% for user in users %}
            <li>
                {{user.id}}  - {{user.firstname}}  -  {{user.lastname}} - {{user.email}} - {{user.birthday|date}} 
            </li>
        {% endfor %}
    </ul>
</div>
{% endblock %}