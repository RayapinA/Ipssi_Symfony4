{% extends 'base.html.twig' %}

{% block title %}Hello UserController!{% endblock %}

{% block body %}

<div class="example-wrapper">
    <h1>La liste des utilisateurs</h1>

    <ul>
        {% for user in users %}
            <li>
                {{user.id}}  - {{user.firstname}}  -  {{user.lastname}} - {{user.email}} - {{user.birthday|date}} 
            </li>
        {% endfor %}
    </ul>
<hr />

</div>
{% endblock %}
